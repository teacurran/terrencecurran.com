    "masonry-layout": "^4.2.2",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.2",
    "postcss-strip-inline-comments": "^0.1.5",
    "sass-loader": "^13.2.0",
    "scss-reset": "^1.2.2",
